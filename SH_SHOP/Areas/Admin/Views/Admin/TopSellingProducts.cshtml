@model IEnumerable<dynamic>

@{
    ViewData["Title"] = "Top Selling Products";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Top Selling Products</h2>

<table class="table">
    <thead>
        <tr>
            <th>Product Name</th>
            <th>Total Sold</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ProductName</td>
                <td>@item.TotalSold</td>
            </tr>
        }
    </tbody>
</table>
