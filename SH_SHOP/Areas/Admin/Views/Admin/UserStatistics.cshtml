@model IEnumerable<dynamic>

@{
    ViewData["Title"] = "User Statistics";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <h2>User Statistics by Role</h2>
    <table class="table">
        <thead>
            <tr>
                <th>Role</th>
                <th>User Count</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.RoleName</td>
                    <td>@item.UserCount</td>
                </tr>
            }
        </tbody>
    </table>

</div>