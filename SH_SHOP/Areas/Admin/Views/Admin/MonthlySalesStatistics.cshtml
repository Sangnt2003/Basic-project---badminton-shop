@model IEnumerable<dynamic>

@{
    ViewData["Title"] = "Monthly Sales Statistics";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Monthly Sales Statistics</h2>

<table class="table">
    <thead>
        <tr>
            <th>Year</th>
            <th>Month</th>
            <th>Order Count</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Year</td>
                <td>@item.Month</td>
                <td>@item.OrderCount</td>
            </tr>
        }
    </tbody>
</table>
