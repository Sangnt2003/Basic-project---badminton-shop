@model IEnumerable<dynamic>

@{
    ViewData["Title"] = "Profit Statistics";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <h2>Monthly Profit Statistics</h2>

    <table class="table">
        <thead>
            <tr>
                <th>Year</th>
                <th>Month</th>
                <th>Total Profit</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Year</td>
                    <td>@item.Month</td>
                    <td>@item.TotalProfit</td>
                </tr>
            }
        </tbody>
    </table>

</div>